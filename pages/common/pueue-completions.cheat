% pueue-completions, common

# Generate completions for Bash
sudo pueue completions bash <_usr_share_bash_completion_completions_pueue_bash>

# Generate completions for Zsh
sudo pueue completions zsh <_usr_share_zsh_site_functions>

# Generate completions for fish
sudo pueue completions fish <_usr_share_fish_completions>
