% jmeter, common

# Run a specific test plan in nongui mode
jmeter --nongui --testfile <path_to_file_jmx>

# Run a test plan in nongui mode using a specific log file
jmeter --nogui --testfile <path_to_file_jmx> --logfile <path_to_logfile_jtl>

# Run a test plan in nongui mode using a specific proxy
jmeter --nongui --testfile <path_to_file_jmx> --proxyHost <127_0_0_1> --proxyPort <8888>

# Run a test plan in nongui mode using a specific JMeter property
jmeter --jmeterproperty <key>='<value>' --nongui --testfile <path_to_file_jmx>
